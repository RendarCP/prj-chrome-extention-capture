{
  "manifest_version": 3,
  "name": "Full Page Screenshot",
  "version": "1.0",
  "description": "Capture full page screenshots",
  "permissions": ["activeTab", "scripting", "downloads"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
